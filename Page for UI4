sus.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
sus.ResetOnSpawn = false

amongus.Name = "among us"
amongus.Parent = sus
amongus.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
amongus.BackgroundTransparency = 1.000
amongus.Position = UDim2.new(0.330827057, 0, 0.102362201, 0)
amongus.Size = UDim2.new(0, 559, 0, 50)
